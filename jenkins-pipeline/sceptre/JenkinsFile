pipeline {
    agent any

    stages {
        stage('Setup') {
            steps {
                echo 'Setup Environment'
                sh 'echo ${PROCEDURE}'
                sh 'echo ${Environment}'
            }
        }
        stage('Generate Config Files') {
            steps {
               echo 'Create Environment'
               sh 'python3 jenkins-pipeline/sceptre/create_environment.py -p ${PROCEDURE} -e ${Environment}'
            }

          }
        stage('Cleanup Environment') {
        steps {
           echo 'Cleanup Environment'
          }
        }
      }

    }

